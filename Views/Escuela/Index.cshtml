@model Escuela

@{
    ViewData["Title"] = "School Information";

    <!-- Layout por defecto -->
    Layout = "_Simple";
}

<h1>Escuela</h1>
<h2>Nombre: @Model.Nombre </h2>
<h2>Tipo de Escuela: @Model.TipoEscuela</h2>
<address>
    <p>Direccion: @Model.Direccion</p>
    <p>Pais: @Model.Pais</p>
    <p>Ciudad: @Model.Ciudad</p>
</address>

<p><strong>Año Fundacion: @Model.AñoDeCreación </strong> </p>

<p>@ViewBag.DynamicThing</p>
<!-- Otra forma -->
<p>@ViewData["DynamicThing"]</p>
